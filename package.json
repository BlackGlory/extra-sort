{
  "name": "extra-sort",
  "version": "0.2.0",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "src"
  ],
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:BlackGlory/extra-sort.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "vitest run",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "bench": "run-s bench:*",
    "bench:sort-numbers-ascending": "tsx benches/sort-numbers-ascending.ts",
    "bench:sort-numbers-descending": "tsx benches/sort-numbers-descending.ts",
    "bench:sort-strings-ascending": "tsx benches/sort-strings-ascending.ts",
    "bench:sort-strings-descending": "tsx benches/sort-strings-descending.ts",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/prelude": "^0.4.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "extra-benchmark": "^0.2.4",
    "husky": "4",
    "npm-run-all": "^4.1.5",
    "return-style": "^3.0.1",
    "rimraf": "^6.1.2",
    "standard-version": "^9.5.0",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "typescript-transform-paths": "^3.5.6",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^4.0.18"
  }
}
